{
  "name": "vue-boilerplate",
  "version": "0.1.0",
  "description": "vue-boilerplate",
  "main": "",
  "scripts": {
    "local": "npm run dev --config=local",
    "dev": "webpack-dev-server -d --hot --env.dev --env.config dev",
    "build": "rimraf dist && webpack -p"
  },
  "author": "Jiang Fengming",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-loader": "6.4.1",
    "babel-preset-env": "1.4.0",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.0",
    "eslint": "3.19.0",
    "eslint-config-enough": "0.2.6",
    "eslint-loader": "1.7.1",
    "eslint-plugin-html": "2.0.1",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "postcss-loader": "1.3.3",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "vue-loader": "11.3.4",
    "vue-template-compiler": "2.2.6",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false
        }
      ],
      "stage-2"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "html"
    ],
    "globals": {
      "DEBUG": false,
      "VERSION": false,
      "CONFIG": false
    }
  },
  "dependencies": {
    "vue": "2.2.6",
    "vue-router": "2.4.0"
  }
}
